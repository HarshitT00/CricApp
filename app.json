{
  "expo": {
    "name": "CricApp",
    "slug": "CricApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.cricapp",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs camera access to mark player attendance."
      }
    },
    "android": {
      "android": {
      "softwareKeyboardLayoutMode": "pan",
      "userInterfaceStyle": "light"
    },
      "package": "com.yourname.cricapp",
      "permissions": ["CAMERA"]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}